mixin checkbox-30(name, id)
	.checkbox-wrapper-30
		span.checkbox
			input(
				type="checkbox"
				id=id
				name=name
			)
			svg
				use.checkbox(xlink:href="#checkbox-30")
		svg(
			style="display:none" 
			xmlns="http://www.w3.org/2000/svg"
			)
			symbol#checkbox-30(viewBox="0 0 22 22")
				path(
					d="M5.5,11.3L9,14.8L20.2,3.3l0,0c-0.5-1-1.5-1.8-2.7-1.8h-13c-1.7,0-3,1.3-3,3v13c0,1.7,1.3,3,3,3h13 c1.7,0,3-1.3,3-3v-13c0-0.4-0.1-0.8-0.3-1.2" 
					stroke="currentColor" 
					fill="none"
					)